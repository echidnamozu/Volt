                           <input class="mz-productoptions-option custom_options" prnames="{{value.stringValue|default(value.value)}}" prcode="{{model.productCode}}" description='{{value.bundledProduct.content.productShortDescription|safe}}' optionprice="{{value.deltaPrice}}" pordprice="{% if model.price.salePrice%} {{model.price.salePrice}}{% else %}{% if model.price.price %}{{model.price.price}}{% else %}{{model.priceRange.lower.price}}{% endif %}{% endif %}" option-type="{{ option.attributeDetail.usageType }}" name="select_bulb2" type="radio" id="{{model.productCode}}{{forloop.index}}_{{value.value}}" data-mz-product-option="{{ option.attributeFQN }}" value="{{value.value}}"><label class="mz-productoptions-label" for="{{model.productCode}}{{forloop.index}}_{{value.value}}">{{value.stringValue|default(value.value)}}{% if value.deltaPrice and value.deltaPrice > 0 %} (<span class="price-labels">{{ value.deltaPrice|currency }} {{ labels.more }}</span>){% endif %} </label>
                         {% endif %}    

                         <div class="description-mobile">
                            
                        </div>
                        <div class="clearfix"></div>
                        </div>

                    {% endfor %}
                {% endif %}
                </div>
                <div class="description">
                    
                </div>
                <div class="clearfix"></div>
         
            {% endif %}
        </div>
        </div>
    {% endif %}
    {% if option.attributeFQN  == themeSettings.BottomCustomisedBulbExtra  %}            
