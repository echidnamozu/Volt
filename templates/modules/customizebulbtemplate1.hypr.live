
 {% for option in model.options %}   
 
    {% if option.attributeFQN  == themeSettings.doubleCustomisedBulbMostPopularExtra %}   

        <div class="mz-productoptions-valuecontainer mostpopular" mostopt="{{option.attributeFQN}}">
        <div class="mz-option-space">
            <h2 class="option-hdr mz-mobile extra-fonts">Most popular bulbs</h2>
            <div class="img-slot">
                <img src="{{ siteContext.cdnPrefix }}/cms/{{siteContext.siteId}}/files/nooption.png" alt="Most popular bulbs"/>
            </div>    
           
            {% if option.attributeDetail.inputType == "List" %}    
            
                <div class="option-container"> 
                    <h2 class="option-hdr mz-desktop extra-fonts">Most popular bulbs</h2>
                {% if pageContext.title == "Quick Order" %}
                    {% for value in option.values %} 
                    
                        <div class="options">  
                        
                        {% if option.attributeDetail.usageType=="Option" %}
                            <input class="mz-productoptions-option custom_options" poption product-code="{{model.productCode}}" prnames="{{value.stringValue|default(value.value)}}" prcode="{{model.productCode}}" description='{{value.bundledProduct.content.productShortDescription|safe}}' pordprice="{% if model.priceRange %} {{model.priceRange.lower.price}} {% else %}{{model.price.price}}{% endif %}" option-type="{{ option.attributeDetail.usageType }}" name="select_bulb" type="radio" id="{{model.productCode}}{{forloop.index}}_{{value.value}}" data-mz-product-option="{{ option.attributeFQN }}" value="{{value.value}}"  optionprice=1 ><label class="mz-productoptions-label" for="{{model.productCode}}{{forloop.index}}_{{value.value}}" >{{value.stringValue|default(value.value)}}{% if value.deltaPrice and value.deltaPrice > 0 %} (<span class="price-labels">{{ value.deltaPrice|currency }} {{ labels.more }}</span>){% endif %} </label> 
                        {% else %}
                           <input class="mz-productoptions-option custom_options" poption product-code="{{model.productCode}}" prnames="{{value.stringValue|default(value.value)}}" prcode="{{model.productCode}}" description='{{value.bundledProduct.content.productShortDescription|safe}}' optionprice="{{value.deltaPrice}}" pordprice="{% if model.price.salePrice%} {{model.price.salePrice}}{% else %}{% if model.price.price %}{{model.price.price}}{% else %}{{model.priceRange.lower.price}}{% endif %}{% endif %}" option-type="{{ option.attributeDetail.usageType }}" name="select_bulb" type="radio" id="{{model.productCode}}{{forloop.index}}_{{value.value}}" data-mz-product-option="{{ option.attributeFQN }}" value="{{value.value}}"><label class="mz-productoptions-label" for="{{model.productCode}}{{forloop.index}}_{{value.value}}">{{value.stringValue|default(value.value)}}{% if value.deltaPrice and value.deltaPrice > 0 %} (<span class="price-labels">{{ value.deltaPrice|currency }} {{ labels.more }}</span>){% endif %} </label>
                         {% endif %}    

                         <div class="description-mobile">   
                            
                        </div>
                        <div class="clearfix"></div>
                        </div>

                    {% endfor %}
                {% else %}
                    {% for value in option.values %} 
                    
                        <div class="options">  
                        
                        {% if option.attributeDetail.usageType=="Option" %}
                            <input class="mz-productoptions-option custom_options" prnames="{{value.stringValue|default(value.value)}}" prcode="{{model.productCode}}" description='{{value.bundledProduct.content.productShortDescription|safe}}' pordprice="{% if model.priceRange %} {{model.priceRange.lower.price}} {% else %}{{model.price.price}}{% endif %}" option-type="{{ option.attributeDetail.usageType }}" name="select_bulb2" type="radio" id="{{model.productCode}}{{forloop.index}}_{{value.value}}" data-mz-product-option="{{ option.attributeFQN }}" value="{{value.value}}"  optionprice=1 ><label class="mz-productoptions-label" for="{{model.productCode}}{{forloop.index}}_{{value.value}}">{{value.stringValue|default(value.value)}}{% if value.deltaPrice and value.deltaPrice > 0 %} (<span class="price-labels">{{ value.deltaPrice|currency }} {{ labels.more }}</span>){% endif %} </label> 
